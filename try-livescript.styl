#ls-console
	word-wrap: break-word;
	font-size: 14px
	font-family: Menlo, monospace

	.jquery-console-inner
		width: 100%
		height:200px
		background:#000
		padding:0.5em
		overflow: auto

	.jquery-console-cursor
		background:#fefefe
		color:#333
		font-weight:bold

	.jquery-console-prompt-box
		color: #fff
		white-space: pre
		tab-size: 4

	.jquery-console-message-value
		color: #1ad027
		white-space: pre

	.jquery-console-message-type
		color: #52666f
		white-space: pre
		padding: 0.1em

	.jquery-console-message-error
		color: rgb(168, 48, 48)
		white-space: pre

	.jquery-console-prompt-label
		font-weight: bold



body
	font-family: Helvetica, Arial, sans-serif
	font-size: 15px

code
	font-family: menlo, monospace
	background-color: rgb(219, 219, 219)
	padding: 0.1ex 0.5em
	font-size: 14px

pre
	tab-size: 4
	font-family: menlo, monospace
	background-color: rgb(219, 219, 219)
	padding: 1ex 0.5em
	font-size: 14px

code.prompt, pre.prompt
	cursor: pointer


section[x-path]
	display: none